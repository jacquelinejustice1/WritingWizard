<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.writingwizard.CreateAccountController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <AnchorPane prefHeight="250.0" prefWidth="1366">
        <Label layoutX="555.0" layoutY="92.0" text="Welcome to">
            <font>
                <Font name="Arial Bold" size="36.0" />
            </font>
        </Label>
        <Label layoutX="346.0" layoutY="134.0" text="WRITING WIZARD">
            <font>
                <Font name="Arial Bold" size="72.0" />
            </font>
            <effect>
                <DropShadow blurType="ONE_PASS_BOX" height="26.86" offsetX="5.0" radius="12.1975" width="23.93">
                    <color>
                        <Color red="0.5215686559677124" green="0.08235294371843338" blue="1.0" />
                    </color>
                </DropShadow>
            </effect>
        </Label>

    </AnchorPane>
    <AnchorPane prefHeight="600.0" prefWidth="1366">
      <children>
         <Label layoutX="616.0" layoutY="22.0" text="Create Account">
            <font>
               <Font name="Arial Bold" size="24.0" />
            </font>
         </Label>
         <TextField fx:id="createUsername" layoutX="582.0" layoutY="81.0" onAction="#createNewUsername" />
         <Label layoutX="582.0" layoutY="66.0" text="Username">
            <font>
               <Font name="Arial Bold" size="12.0" />
            </font>
         </Label>
         <TextField fx:id="reenterCreateUsername" layoutX="582.0" layoutY="133.0" onAction="#createUsernameReenter" />
         <Label layoutX="582.0" layoutY="118.0" text="Re-enter Username">
            <font>
               <Font name="Arial Bold" size="12.0" />
            </font>
         </Label>
         <PasswordField fx:id="createPassword" layoutX="582.0" layoutY="184.0" onAction="#createPassword" />
         <Label layoutX="583.0" layoutY="169.0" text="Password">
            <font>
               <Font name="Arial Bold" size="12.0" />
            </font></Label>
         <PasswordField fx:id="createPasswordReenter" layoutX="582.0" layoutY="234.0" onAction="#createPasswordReenter" />
         <Label layoutX="583.0" layoutY="218.0" text="Re-enter password">
            <font>
               <Font name="Arial Bold" size="12.0" />
            </font>
         </Label>
         <TextField fx:id="firstPet" layoutX="584.0" layoutY="289.0" onAction="#saveFirstPetQuestion" prefHeight="25.0" prefWidth="198.0" />
         <Label layoutX="584.0" layoutY="274.0" text="What is the name of your first pet?">
            <font>
               <Font name="Arial Bold" size="12.0" />
            </font>
         </Label>
         <TextField fx:id="motherName" layoutX="584.0" layoutY="340.0" onAction="#saveMotherName" prefHeight="25.0" prefWidth="213.0" />
         <Label layoutX="585.0" layoutY="323.0" text="What is your mother's maiden name?">
            <font>
               <Font name="Arial Bold" size="12.0" />
            </font>
         </Label>
         <TextField fx:id="elementaryName" layoutX="585.0" layoutY="391.0" prefHeight="25.0" prefWidth="272.0" />
         <Label layoutX="585.0" layoutY="376.0" text="What was the name of your elementary school?">
            <font>
               <Font name="Arial Bold" size="12.0" />
            </font>
         </Label>
         <Button layoutX="741.0" layoutY="82.0" mnemonicParsing="false" style="-fx-background-color: #8515FF;" text="Submit" textFill="WHITE">
            <font>
               <Font name="Arial Bold" size="13.0" />
            </font>
         </Button>
         <Button layoutX="741.0" layoutY="133.0" mnemonicParsing="false" style="-fx-background-color: #8515FF;" text="Submit" textFill="WHITE">
            <font>
               <Font name="Arial Bold" size="13.0" />
            </font>
         </Button>
         <Button layoutX="741.0" layoutY="185.0" mnemonicParsing="false" style="-fx-background-color: #8515FF;" text="Submit" textFill="WHITE">
            <font>
               <Font name="Arial Bold" size="13.0" />
            </font>
         </Button>
         <Button layoutX="741.0" layoutY="235.0" mnemonicParsing="false" style="-fx-background-color: #8515FF;" text="Submit" textFill="WHITE">
            <font>
               <Font name="Arial Bold" size="13.0" />
            </font>
         </Button>
         <Button layoutX="793.0" layoutY="290.0" mnemonicParsing="false" style="-fx-background-color: #8515FF;" text="Submit" textFill="WHITE">
            <font>
               <Font name="Arial Bold" size="13.0" />
            </font>
         </Button>
         <Button layoutX="808.0" layoutY="340.0" mnemonicParsing="false" style="-fx-background-color: #8515FF;" text="Submit" textFill="WHITE">
            <font>
               <Font name="Arial Bold" size="13.0" />
            </font>
         </Button>
         <Button layoutX="868.0" layoutY="392.0" mnemonicParsing="false" style="-fx-background-color: #8515FF;" text="Submit" textFill="WHITE">
            <font>
               <Font name="Arial Bold" size="13.0" />
            </font>
         </Button>
         <Button fx:id="submitAccount" layoutX="591.0" layoutY="435.0" mnemonicParsing="false" onAction="#submitAccountInfo" prefHeight="25.0" prefWidth="228.0" style="-fx-background-color: #8515FF;" text="CREATE ACCOUNT" textFill="WHITE">
            <font>
               <Font name="Arial Bold" size="20.0" />
            </font>
         </Button>

      </children></AnchorPane>


</VBox>
